<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Trato.Views.V_MedicoVista">
    <ContentPage.Content>
        <StackLayout Orientation="Vertical" Margin="70,100,70,0" BackgroundColor="Red">
            <Image Source="xamarin_logo.png"  BackgroundColor="Black"></Image>
            <Label x:Name="nombre" Margin="0,5,0,5"></Label>
            <Label x:Name="especial" Margin="0,5,0,5"></Label>
            <Label x:Name="domicilio" Margin="0,5,0,5"></Label>
            <Label x:Name="info" Margin="0,5,0,5"></Label>
            <Button  Clicked="Fn_Click" x:Name="boton" Margin="0,5,0,5"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>